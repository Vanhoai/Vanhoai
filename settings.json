{
    "editor.fontFamily": "JetBrainsMonoNL Nerd Font",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.tabSize": 4,
    "window.zoomLevel": 0.6,
    "workbench.tree.indent": 16,
    "debug.openDebug": "openOnDebugBreak",
    "debug.internalConsoleOptions": "openOnSessionStart",
    "editor.inlayHints.enabled": "offUnlessPressed",
    "files.exclude": {
        "USE_GITIGNORE": true,
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/coverage": true,
        "**/dist": true,
        "**/build": true,
        "**/.build": true,
        "**/cmake-build*": true,
        "**/.cache": true,
        "**/.idea": true,
    },
    "files.watcherExclude": {
        "**/node_modules": true,
        "**/coverage": true,
        "**/dist": true,
        "**/build": true,
        "**/.build": true,
        "**/cmake-build*": true,
        "**/.cache": true,
        "**/.idea": true,
    },
    "editor.codeActionsOnSave": {
        "source.organizeImports": "always",
        "source.fixAll": "explicit"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
    "terminal.integrated.fontLigatures.enabled": true,
    "terminal.integrated.fontSize": 13,
    "git.autofetch": true,
    "terminal.integrated.cursorWidth": 4,
    "git.confirmSync": false,
    "explorer.compactFolders": false,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.formatOnSave": true,
    "editor.smoothScrolling": true,
    "terminal.integrated.suggest.enabled": true,
    "terminal.integrated.inheritEnv": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "workbench.editorAssociations": {
        "*.txt": "default"
    },
    "terminal.integrated.commandsToSkipShell": [
        "matlab.interrupt"
    ],
    "python.analysis.typeCheckingMode": "standard",
    "extensions.ignoreRecommendations": true,
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "workbench.colorTheme": "Tokyo Night",
    "workbench.iconTheme": "charmed-icons",
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false
    },
    "cSpell.userWords": [
        "aurorastudyvn",
        "avgpool",
        "backpropagation",
        "cifar",
        "clangd",
        "convolutional",
        "crossentropy",
        "csignature",
        "CUDA",
        "Depthwise",
        "dijsktra",
        "drogon",
        "Eigen",
        "endfunction",
        "fenwick",
        "figsize",
        "finetune",
        "Hinsun",
        "Hoai",
        "imagenet",
        "imread",
        "imshow",
        "imwrite",
        "jsonable",
        "jsonwebtoken",
        "keepdims",
        "keras",
        "keymaps",
        "kosaraju",
        "kruskal",
        "lamda",
        "lazypath",
        "loadtxt",
        "matplotlib",
        "maxn",
        "maxpool",
        "MNIST",
        "multiclass",
        "nesterov",
        "numpy",
        "opencv",
        "Overfitting",
        "Pointwise",
        "pysignature",
        "rabbitmq",
        "randn",
        "rcnn",
        "scikit",
        "sklearn",
        "Sobel",
        "stdpath",
        "tensorboard",
        "Thresholding",
        "tokyonight",
        "torchinfo",
        "torchvision",
        "underfitting",
        "unvectorized",
        "usecases"
    ],
    "editor.copyWithSyntaxHighlighting": false,
    "workbench.editor.enablePreview": false,
    "window.newWindowDimensions": "inherit",
    "editor.multiCursorModifier": "ctrlCmd",
    "files.trimTrailingWhitespace": true,
    "diffEditor.renderSideBySide": false,
    "editor.snippetSuggestions": "top",
    "editor.detectIndentation": false,
    "window.nativeFullScreen": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.minimap.enabled": false,
    "editor.lineNumbers": "off",
    "editor.guides.indentation": false,
    // Siliene settings
    "breadcrumbs.enabled": false,
    "scm.diffDecorations": "none",
    "editor.hover.delay": 1500,
    "editor.hover.enabled": true,
    "editor.matchBrackets": "never",
    "workbench.tips.enabled": false,
    "editor.colorDecorators": false,
    "git.decorations.enabled": false,
    "workbench.startupEditor": "none",
    "editor.lightbulb.enabled": "off",
    "editor.selectionHighlight": false,
    "editor.overviewRulerBorder": false,
    "editor.renderLineHighlight": "none",
    "editor.occurrencesHighlight": "off",
    "problems.decorations.enabled": false,
    "editor.renderControlCharacters": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleDeclarations": "goto",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",
    "workbench.statusBar.visible": false,
    "vscode_custom_css.imports": [
        "file:///Users/hinsun/Workspace/Vanhoai/.config/vscode/vscode-styles.css",
        "file:///Users/hinsun/Workspace/Vanhoai/.config/vscode/vscode-scripts.js"
    ],
}
